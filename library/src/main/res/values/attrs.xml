<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TagFlowLayout">
        <attr name="showHighlight" format="boolean"/>
        <attr name="defaultDrawable" format="reference"/>
        <attr name="selectedDrawable" format="reference"/>
        <attr name="defaultTextColor" format="color|reference"/>
        <attr name="selectedTextColor" format="color|reference"/>
        <attr name="selectMode">
            <enum name="multi" value="0"/>
            <enum name="single" value="1"/>
        </attr>
        <attr name="maxSelectionCount" format="integer"/>
    </declare-styleable>

    <declare-styleable name="AppreciateView">
        <attr name="image_size" format="dimension"/>
        <attr name="image_background" format="reference"/>
        <attr name="text_color" format="color"/>
        <attr name="text_size" format="dimension"/>
        <attr name="text" format="string"/>
        <attr name="uncheck_disable" format="boolean"/>
    </declare-styleable>
</resources>